<!-- views/pages/index.ejs -->
<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include partials/head.ejs %>
    </head>
    <body class="container">

        <header>
            <% include partials/header %>
        </header>

        <main>
            <div class="jumbotron">
                <h1>TinyApp</h1>
                <p>A bit.ly-like URL shortener.</p>
            </div>

            <div>
            <div class="table-responsive">
              <table class="table">
                <% for (let url in urls ) { %>
                    <tr>
                        <td><%- url%></td>
                        <td><%-urls[url]%></td>
                    </tr>
                <% } %>
              </table>
            </div>


            </div>
        </main>

        <footer>
             <% include partials/footer %>
        </footer>

    </body>
</html>